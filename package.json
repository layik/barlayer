{
  "name": "barlayer",
  "description": "Custom DeckGL layer of Washington Post visualization of bars.",
  "version": "0.0.1",
  "main": "dist/index.js",
  "author": "L Hama (https://github.com/layik)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/layik/"
  },
  "files": [
    "README.md",
    "dist/"
  ],
  "scripts": {
    "dist": "rm -rf dist && NODE_ENV=production node_modules/.bin/babel src/ --out-dir dist --no-copy-ignored",
    "watch": "watch 'yarn run dist' ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "externals": {
    "react": "commonjs react",
    "react-dom": "commonjs react-dom"
  },
  "output": {
    "libraryTarget": "commonjs2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "deck.gl": "^8.3.15",
    "@luma.gl/addons": "^7.3.2",
    "@luma.gl/debug": "^8.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "watch": "^1.0.2",
    "webpack-cli": "^3.3.12"
  }
}
